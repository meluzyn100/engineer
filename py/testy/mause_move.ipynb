    "from time import sleep\n",
    "import pyautogui\n",
    "import numpy as np\n",
    "\n",
    "# Check your screen size\n",
    "print(pyautogui.size())\n",
    "\n",
    "count=0\n",
    "while 1:\n",
    "     x=np.random.randint(800,1400)\n",
    "     y=np.random.randint(500,600)\n",
    "     pyautogui.moveTo(x, y)\n",
    "     print(x, y)\n",
    "     sleep(20)\n",
    "     count+=1"
   ]
  }
